<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Home</title>
  </head>
  <body>
    <% videos.forEach(element => { %>
        <% if (element != "thumbs") { %>
            <a href="<%- "/videos/" + element %> "><img height="500" width="500" src="<%- "/thumbnail/" + element %> " loading="lazy"></a>
        <% } %>
    
    <% }) %>
  </body>
</html>
